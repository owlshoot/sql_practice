#Query to calculate the average number of cases by continent
Select continent,  Round(avg(activecases)) as Avg_cases_per_continent FROM covid.worldometer_data
group by continent
order by 2 desc;

+-------------------+-------------------------+
| continent         | Avg_cases_per_continent |
+-------------------+-------------------------+
| North America     |                  149042 |
| South America     |                   97864 |
| Asia              |                   34818 |
| Europe            |                   15963 |
| Africa            |                   10213 |
| Australia/Oceania |                    8694 |
+-------------------+-------------------------+

#Query to calculate the increase in confirmed cases by day as well as its increase in percentage
SELECT  date, confirmed,
	confirmed - LAG (confirmed) OVER (ORDER BY date ) AS number_change,
	(confirmed - LAG (confirmed) OVER (ORDER BY date ))/LAG (confirmed) OVER (ORDER BY date )*100 as percentage_change
From day_wise;
